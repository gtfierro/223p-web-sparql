            sh:name "ConnectionPoint direction" ;
            sh:or ( [ sh:hasValue s223:Direction-Inlet ] [ sh:hasValue s223:Direction-Outlet ] [ sh:hasValue s223:Direction-Bidirectional ] ) ;
